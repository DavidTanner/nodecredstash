    "@vitest/coverage-v8": "^0.34.6",
    "aws-sdk-client-mock": "^3.0.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-chai-expect": "^3.0.0",
